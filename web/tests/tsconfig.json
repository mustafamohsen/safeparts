{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./tests-dist",
    "rootDir": "./tests",
    "types": ["@playwright/test"]
  },
  "include": ["tests/**/*"],
  "exclude": ["node_modules"]
}
